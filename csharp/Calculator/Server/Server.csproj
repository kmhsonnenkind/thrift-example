<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp5.0</TargetFramework>
    <LangVersion>8</LangVersion>
    <Nullable>enable</Nullable>
    <Version>0.0.1</Version>
    <Authors>Martin Kloesch</Authors>
    <Company>kmh-solutions</Company>
    <Product>Thrift Example</Product>
    <Description>Example server for Apache Thrift service</Description>
    <Copyright>2021 Martin Kloesch</Copyright>
    <PackageLicenseUrl>https://github.com/kmhsonnenkind/thrift-example/blob/main/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/kmhsonnenkind/thrift-example</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kmhsonnenkind/thrift-example.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>example thrift server</PackageTags>
    <RootNamespace>Calculator.Server</RootNamespace>
    <AssemblyName>Calculator.Server</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Calculator\Calculator.csproj" />
  </ItemGroup>

